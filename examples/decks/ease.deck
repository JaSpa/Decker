{script:home.0}
on view do
 names.value:insert "Function Name":keys ease into 0
 name:first names.rowvalue
 
 usage.value:rtext.cat[
  rtext.make["usage:\n\n  "]
  rtext.make["ease.%s[time a b]" format name "mono"]
 ]
 
 m:20
 i:c.size-2*m
 f:ease[name]
 c.clear[]
 c.brush:4
 c.pattern:13
 c.line[each t in range i[0]
  (m+t),(f[t/i[0] m+i[1] m])
 end]
 c.brush:0
 c.pattern:1
 c.line[m,m m,c.size[1]-m c.size-m]
end
{end}

